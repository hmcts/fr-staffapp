.govuk-form-group
  = f.label :office_id, class: 'govuk-label'
  span.govuk-error-message
    = f.label :office_id, @user.errors[:office_id].join(', ').html_safe, class: 'error' if @user.errors[:office_id].present?
  = f.collection_select :office_id, @offices, :id, :name, {}, { class: 'govuk-select' }
